    "type": "git",
    "url": "https://github.com/jhades/angular2-redux-store.git"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.14",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.5",
    "zone.js": "^0.6.6"
  },
  "devDependencies": {
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
